#1004

#include <stdio.h>

int main() {
	printf("\'Hello\'");            //printf('')는 오류뜸 / #printf("")로 해줘야

	return 0;
} 

#1005
#include <stdio.h>

int main() {
	printf("\"Hello World\"");

	return 0;
} 

//1006번
#include <stdio.h>

int main() 
{
	printf("\"!@#$%%^&*()\"");                 //  %%의 출력

	return 0;
}


//1007 백슬래시 \ 의 출력은 \\로
