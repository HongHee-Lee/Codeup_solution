import java.util.Scanner;

public class Main{
	public static void main(String[] args) {

		Scanner sc= new Scanner(System.in);
		String input = sc.nextLine();                               // nextLine 의 위력을 체감할수 있는 문제
		
		System.out.printf(input);
	}
}
