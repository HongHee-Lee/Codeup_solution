import java.util.Scanner;

public class Main{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);	
		
		int n= sc.nextInt();
		int i=0;
		while(i<n)
		{
			int input= sc.nextInt();
			System.out.println(input);
			i++;
		}

	}	
}
