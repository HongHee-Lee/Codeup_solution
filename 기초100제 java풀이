단, -2147483648 ~ +2147483647

import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		long a = sc.nextLong();                                               //요기 부분 long대신 int로 해줘도 됨
		long b = sc.nextLong();
		long c = sc.nextLong();
		
		System.out.println(a+b+c);
		System.out.printf("%.1f", (float)((a+b+c)/3));                     //(float)((a+b+c)/3) 일케 하면 오류뜨고
                                                                                   //(float)(a+b+c)/3  해주면 오류안뜸
	}
}
