import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		char a = sc.next().charAt(0);                //sc.next().charAt(0); 문자 하나만 입력받는 방법
		
		int b=(int)a;                                // (int)a - Ascii code number 변환 방법
		
		System.out.println(b);
		

	}
}
