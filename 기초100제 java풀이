import java.util.Scanner;

public class Main{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		String n = Integer.toHexString(sc.nextInt()).toUpperCase();                   //toUpperCase() 문자열을 대문자로 변환 (내장함수)
		System.out.println(n);                                                        //toLowerCase() 문자열을 소문자로 변환 (내장함수)
	}
