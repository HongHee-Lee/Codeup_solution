import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		
		String data[]=new String[51];                 //배열 만드는 법 공부
				
		Scanner sc = new Scanner(System.in);
		data[50]= sc.next(); 
		
		System.out.println(data[50]);
		
		
		
	}
}
