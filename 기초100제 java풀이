import java.util.Scanner;

public class Main{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.printf("%x", n);                                 //%x로 출력하면 16진수(hexadecimal) 소문자로 출력된다.
	}
}
