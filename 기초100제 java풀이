*************************공부***************************
import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();                                  //String s의 길이를 알아보자.
		System.out.println(s.length());				   //s='today is Tuesday'     ---->   s.length() 는 16임(공백포함)
	}

}
********************************************************


import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();                          //nextLine으로 받아야 today is Tuesday가 온전히 받아짐   //next()는 today까지만 받아짐
		String arr[]= s.split(" ");                       //리스트 만들어주는 (배열 생성)코드         String arr[]    -    String[] arr        둘다 eclipse에서 실행가능
		for(int i=0; i<=s.length(); i++)
		System.out.println(arr[i]);
	}
}
