import java.util.Scanner;

public class Main{
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();                                      //위에 코드는 String n , 지금 코드는 int n 으로 시작하는거 잘보기
		System.out.println(Integer.toHexString(n));                     //Integer.toHexString(n)       16진수로 표현하기
	}
}
255
ff



