public class Main{
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		String n = Integer.toHexString(sc.nextInt());          //toHexString이 String으로 만들어주는 건가봐. Int n으로 하면 cannot convert from String to int 라고함            
		System.out.println(n);
	}
}
