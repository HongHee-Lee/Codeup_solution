import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		
	Scanner sc = new Scanner(System.in);
	String str = sc.next();
	String arr[]=str.split("[-&/*,]");                                    //split("[-&/*,]")     새로운 표현 공부\!           \\을 넣으면 \으로 안잘림 why?
		int front = Integer.parseInt(arr[0]);                         //parseInt             새로운 표현 공부!
		int back = Integer.parseInt(arr[1]);
	
	System.out.printf("%06d%07d",front,back);
	}
}

